{
  "name": "@brightspace-ui-labs/consistent-evaluation",
  "description": "A consistent evaluation page for all tools",
  "repository": "https://github.com/BrightspaceHypermediaComponents/consistent-evaluation.git",
  "private": true,
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:lit",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer consistent-evaluation.js demo test",
    "start": "es-dev-server --app-index demo/index.html --node-resolve --dedupe --open --watch",
    "test": "npm run lint && npm run test:headless",
    "test:headless": "karma start",
    "test:sauce": "karma start karma.sauce.conf.js"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@open-wc/testing": "^2",
    "@open-wc/testing-karma": "^3",
    "babel-eslint": "^10",
    "deepmerge": "^3",
    "es-dev-server": "^1",
    "eslint": "^6",
    "eslint-config-brightspace": "^0.6.6",
    "eslint-plugin-html": "^6",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "frau-ci": "^1",
    "karma-sauce-launcher": "^2",
    "lit-analyzer": "^1",
    "puppeteer": "^1"
  },
  "dependencies": {
    "@brightspace-ui/core": "^1",
    "@webcomponents/webcomponentsjs": "^2",
    "lit-element": "^2"
  }
}
